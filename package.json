    "less-loader": "~2.2.0",
    "moment": "~2.8.3",
    "moment-timezone": "~0.3.1",
    "numeral": "~1.5.3",
    "object-assign": "^1.0.0",
    "prettycron": "~0.10.0",
    "rc-switch": "^1.3.3",
    "react": "~0.13.3",
    "react-autosuggest": "git+https://github.com/keboola/react-autosuggest",
